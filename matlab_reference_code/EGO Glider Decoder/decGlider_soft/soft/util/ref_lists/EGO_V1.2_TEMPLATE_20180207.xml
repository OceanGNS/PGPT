<?xml version="1.0" encoding="UTF-8"?>
<RULES>
	<APPLICABLE_IF>
		<GLOBAL_ATTRIBUTE NAME="data_type" VALUE="EGO glider time-series data"/>
		<GLOBAL_ATTRIBUTE NAME="format_version" VALUE="1.2"/>
	</APPLICABLE_IF>

	<MANDATORY>
		<!-- ==================== -->
		<!-- Mandatory dimensions -->
		<!-- ==================== -->
		<DIMENSION NAME="TIME"/>
		<DIMENSION NAME="TIME_GPS"/>

		<!-- =========================== -->
		<!-- Mandatory global attributes -->
		<!-- =========================== -->
		
		<!-- What -->
		<GLOBAL_ATTRIBUTE NAME="data_type" VALUE="EGO glider time-series data"/>
		<GLOBAL_ATTRIBUTE NAME="format_version" VALUE="1.2"/>
		<GLOBAL_ATTRIBUTE NAME="platform_code"/>
		<GLOBAL_ATTRIBUTE NAME="date_update" PATTERN="[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(\.[0-9]+)?Z"/>
		<GLOBAL_ATTRIBUTE NAME="data_mode" PATTERN="R|P|D|M"/>
		<GLOBAL_ATTRIBUTE NAME="naming_authority" VALUE="EGO"/>
		<GLOBAL_ATTRIBUTE NAME="id"/>

		<!-- =================== -->
		<!-- Mandatory variables -->
		<!-- =================== -->
		
		<!-- Coordinate variables -->
		<!-- =================== -->
		
		<!-- TIME -->
		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="TIME"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="double"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME" RANK="1"/>
				<ATTRIBUTE NAME="long_name" VALUE="Epoch time"/>
				<ATTRIBUTE NAME="standard_name" VALUE="time"/>
				<ATTRIBUTE NAME="units" VALUE="seconds since 1970-01-01T00:00:00Z"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="9999999999"/>
				<ATTRIBUTE NAME="valid_min" VALUE="0"/>
				<ATTRIBUTE NAME="valid_max" VALUE="90000"/>
				<ATTRIBUTE NAME="QC_procedure"/>
				<!-- <OPTIONALATTRIBUTE NAME="comment"/> -->
				<ATTRIBUTE NAME="axis" VALUE="T"/>
				<ATTRIBUTE NAME="ancillary_variable" VALUE="TIME_QC"/>
				<ATTRIBUTE NAME="sdn_parameter_urn" VALUE="SDN:P01::ELTMEP01"/>
				<ATTRIBUTE NAME="sdn_uom_urn" VALUE="SDN:P061::UTBB"/>
				<ATTRIBUTE NAME="glider_original_parameter_name"/>
			</MUST_VERIFY>
		</VARIABLE>

		<!-- JULD -->
		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="JULD"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="double"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME" RANK="1"/>
				<ATTRIBUTE NAME="long_name" VALUE="Julian 1950 time"/>
				<ATTRIBUTE NAME="standard_name" VALUE="time"/>
				<ATTRIBUTE NAME="units" VALUE="days since 1950-01-01T00:00:00Z"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="999999"/>
				<ATTRIBUTE NAME="valid_min" VALUE="0"/>
				<ATTRIBUTE NAME="valid_max" VALUE="90000"/>
				<ATTRIBUTE NAME="QC_procedure"/>
				<!-- <ATTRIBUTE NAME="comment"/> -->
				<ATTRIBUTE NAME="axis" VALUE="T"/>
				<ATTRIBUTE NAME="ancillary_variable" VALUE="JULD_QC"/>
				<!-- <ATTRIBUTE NAME="sdn_parameter_urn" VALUE="SDN:P01::XXX"/> -->
				<ATTRIBUTE NAME="sdn_parameter_urn"/>
				<ATTRIBUTE NAME="sdn_uom_urn" VALUE="SDN:P061::UTAA"/>
				<ATTRIBUTE NAME="glider_original_parameter_name"/>
			</MUST_VERIFY>
		</VARIABLE>
		
		<!-- LATITUDE -->
		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="LATITUDE"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="double"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME" RANK="1"/>	
				<ATTRIBUTE NAME="long_name" VALUE="Measurement latitude"/>
				<ATTRIBUTE NAME="standard_name" VALUE="latitude"/>
				<ATTRIBUTE NAME="units" VALUE="degree_north"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="99999"/>
				<ATTRIBUTE NAME="valid_min" VALUE="-90"/>
				<ATTRIBUTE NAME="valid_max" VALUE="90"/>
				<ATTRIBUTE NAME="QC_procedure"/>
				<!-- <ATTRIBUTE NAME="comment"/> -->
				<ATTRIBUTE NAME="axis" VALUE="Y"/>
				<ATTRIBUTE NAME="ancillary_variable" VALUE="POSITION_QC"/>
				<ATTRIBUTE NAME="reference" VALUE="WGS84"/>							
				<ATTRIBUTE NAME="coordinate_reference_frame" VALUE="urn:ogc:crs:EPSG::4326"/>	
				<ATTRIBUTE NAME="sdn_parameter_urn" VALUE="SDN:P01::ALATZZ01"/>
				<ATTRIBUTE NAME="sdn_uom_urn" VALUE="SDN:P061::DEGN"/>
				<ATTRIBUTE NAME="glider_original_parameter_name"/>
			</MUST_VERIFY>
		</VARIABLE>
		
		<!-- LONGITUDE -->
		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="LONGITUDE"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="double"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME" RANK="1"/>	
				<ATTRIBUTE NAME="long_name" VALUE="Measurement longitude"/>
				<ATTRIBUTE NAME="standard_name" VALUE="longitude"/>
				<ATTRIBUTE NAME="units" VALUE="degree_east"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="99999"/>
				<ATTRIBUTE NAME="valid_min" VALUE="-180"/>
				<ATTRIBUTE NAME="valid_max" VALUE="180"/>
				<ATTRIBUTE NAME="QC_procedure"/>
				<!-- <ATTRIBUTE NAME="comment"/> -->
				<ATTRIBUTE NAME="axis" VALUE="X"/>				
				<ATTRIBUTE NAME="ancillary_variable" VALUE="POSITION_QC"/>						
				<ATTRIBUTE NAME="reference" VALUE="WGS84"/>							
				<ATTRIBUTE NAME="coordinate_reference_frame" VALUE="urn:ogc:crs:EPSG::4326"/>
				<ATTRIBUTE NAME="sdn_parameter_urn" VALUE="SDN:P01::ALONZZ01"/>
				<ATTRIBUTE NAME="sdn_uom_urn" VALUE="SDN:P061::DEGE"/>
				<ATTRIBUTE NAME="glider_original_parameter_name"/>
			</MUST_VERIFY>
		</VARIABLE>			
		
		<!-- QC variables -->
		<VARIABLE>
			<IDENTIFY>
				<NAME PATTERN="TIME_QC|JULD_QC|POSITION_QC"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="byte"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME" RANK="1"/>
				<ATTRIBUTE NAME="long_name" VALUE="Quality flag"/>
				<ATTRIBUTE NAME="conventions" VALUE="EGO reference table 2"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="-128"/>
				<ATTRIBUTE NAME="flag_values" VALUE_LIST="0||1||2||3||4||5||8||9"/>
				<ATTRIBUTE NAME="flag_meanings" VALUE="no_qc_performed good_data probably_good_data bad_data_that_are_potentially_correctable bad_data value_changed interpolated_value missing_value"/>
			</MUST_VERIFY>
		</VARIABLE>
		
		<!-- TIME_GPS -->
		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="TIME_GPS"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="double"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME_GPS" RANK="1"/>
				<ATTRIBUTE NAME="long_name" VALUE="Epoch time of the GPS fixes"/>
				<ATTRIBUTE NAME="standard_name" VALUE="time"/>
				<ATTRIBUTE NAME="units" VALUE="seconds since 1970-01-01T00:00:00Z"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="-1"/>
				<ATTRIBUTE NAME="valid_min" VALUE="0"/>
				<ATTRIBUTE NAME="valid_max" VALUE="90000"/>
				<ATTRIBUTE NAME="QC_procedure"/>
				<!-- <ATTRIBUTE NAME="comment"/> -->
				<ATTRIBUTE NAME="axis" VALUE="T"/>
				<ATTRIBUTE NAME="ancillary_variable" VALUE="TIME_GPS_QC"/>
				<ATTRIBUTE NAME="sdn_parameter_urn" VALUE="SDN:P01::ELTMEP01"/>
				<ATTRIBUTE NAME="sdn_uom_urn" VALUE="SDN:P061::UTBB"/>
				<ATTRIBUTE NAME="glider_original_parameter_name"/>
			</MUST_VERIFY>
		</VARIABLE>	
		
		<!-- LATITUDE_GPS -->
		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="LATITUDE_GPS"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="double"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME_GPS" RANK="1"/>	
				<ATTRIBUTE NAME="long_name" VALUE="Gps fixed latitude"/>
				<ATTRIBUTE NAME="standard_name" VALUE="latitude"/>
				<ATTRIBUTE NAME="units" VALUE="degree_north"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="99999"/>
				<ATTRIBUTE NAME="valid_min" VALUE="-90"/>
				<ATTRIBUTE NAME="valid_max" VALUE="90"/>
				<ATTRIBUTE NAME="QC_procedure"/>
				<!-- <ATTRIBUTE NAME="comment"/> -->
				<ATTRIBUTE NAME="axis" VALUE="Y"/>
				<ATTRIBUTE NAME="ancillary_variable" VALUE="POSITION_GPS_QC"/>
				<ATTRIBUTE NAME="reference" VALUE="WGS84"/>							
				<ATTRIBUTE NAME="coordinate_reference_frame" VALUE="urn:ogc:crs:EPSG::4326"/>	
				<ATTRIBUTE NAME="sdn_parameter_urn" VALUE="SDN:P01::ALATZZ01"/>
				<ATTRIBUTE NAME="sdn_uom_urn" VALUE="SDN:P061::DEGN"/>
				<ATTRIBUTE NAME="glider_original_parameter_name"/>
			</MUST_VERIFY>
		</VARIABLE>
		
		<!-- LONGITUDE_GPS -->
		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="LONGITUDE_GPS"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="double"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME_GPS" RANK="1"/>	
				<ATTRIBUTE NAME="long_name" VALUE="Gps fixed longitude"/>
				<ATTRIBUTE NAME="standard_name" VALUE="longitude"/>
				<ATTRIBUTE NAME="units" VALUE="degree_east"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="99999"/>
				<ATTRIBUTE NAME="valid_min" VALUE="-180"/>
				<ATTRIBUTE NAME="valid_max" VALUE="180"/>
				<ATTRIBUTE NAME="QC_procedure"/>
				<!-- <ATTRIBUTE NAME="comment"/> -->
				<ATTRIBUTE NAME="axis" VALUE="X"/>				
				<ATTRIBUTE NAME="ancillary_variable" VALUE="POSITION_GPS_QC"/>
				<ATTRIBUTE NAME="reference" VALUE="WGS84"/>							
				<ATTRIBUTE NAME="coordinate_reference_frame" VALUE="urn:ogc:crs:EPSG::4326"/>
				<ATTRIBUTE NAME="sdn_parameter_urn" VALUE="SDN:P01::ALONZZ01"/>
				<ATTRIBUTE NAME="sdn_uom_urn" VALUE="SDN:P061::DEGE"/>
				<ATTRIBUTE NAME="glider_original_parameter_name"/>
			</MUST_VERIFY>
		</VARIABLE>	
		
		<!-- QC variables -->
		<VARIABLE>
			<IDENTIFY>
				<NAME PATTERN="TIME_GPS_QC|POSITION_GPS_QC"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="byte"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME_GPS" RANK="1"/>
				<ATTRIBUTE NAME="long_name" VALUE="Quality flag"/>
				<ATTRIBUTE NAME="conventions" VALUE="EGO reference table 2"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="-128"/>
				<ATTRIBUTE NAME="flag_values" VALUE_LIST="0||1||2||3||4||5||8||9"/>
				<ATTRIBUTE NAME="flag_meanings" VALUE="no_qc_performed good_data probably_good_data bad_data_that_are_potentially_correctable bad_data value_changed interpolated_value missing_value"/>
			</MUST_VERIFY>
		</VARIABLE>

	</MANDATORY>	

	<OPTIONAL>
	
		<!-- =================== -->
		<!-- Optional dimensions -->
		<!-- =================== -->
		<DIMENSION NAME="TIME_CURRENT"/>		
		<DIMENSION NAME="DATE_TIME" VALUE="14"/>
		<DIMENSION NAME="STRING1024" VALUE="1024"/>
		<DIMENSION NAME="STRING256" VALUE="256"/>
		<DIMENSION NAME="STRING64" VALUE="64"/>
		<DIMENSION NAME="STRING32" VALUE="32"/>
		<DIMENSION NAME="STRING16" VALUE="16"/>
		<DIMENSION NAME="STRING8" VALUE="8"/>
		<DIMENSION NAME="STRING4" VALUE="4"/>
		<DIMENSION NAME="STRING2" VALUE="2"/>
		<DIMENSION NAME="N_HISTORY"/>
		<DIMENSION NAME="N_PARAM"/>
		<DIMENSION NAME="N_DERIVATION"/>
		<DIMENSION NAME="N_POSITIONING_SYSTEM"/>
		<DIMENSION NAME="N_TRANS_SYSTEM"/>

		<!-- ========================== -->
		<!-- Optional global attributes -->
		<!-- ========================== -->
		
		<!-- What -->
		<GLOBAL_ATTRIBUTE NAME="wmo_platform_code"/>
		<GLOBAL_ATTRIBUTE NAME="ices_platform_code"/>
		<GLOBAL_ATTRIBUTE NAME="source" PATTERN="Shipborne observation|Glider observation"/>
		<GLOBAL_ATTRIBUTE NAME="history"/>
		<GLOBAL_ATTRIBUTE NAME="quality_index" PATTERN="unknown quality|excellent|probably good|extremely suspect"/>
		<GLOBAL_ATTRIBUTE NAME="references" VALUE="http://www.ego-network.org/"/>
		<GLOBAL_ATTRIBUTE NAME="comment"/>
		<GLOBAL_ATTRIBUTE NAME="Conventions" VALUE="CF-1.4 EGO-1.2"/>
		<GLOBAL_ATTRIBUTE NAME="netcdf_version"/>
		<GLOBAL_ATTRIBUTE NAME="title"/>
		<GLOBAL_ATTRIBUTE NAME="summary"/>
		<GLOBAL_ATTRIBUTE NAME="abstract"/>
		<GLOBAL_ATTRIBUTE NAME="keywords"/>
		<GLOBAL_ATTRIBUTE NAME="cdm_data_type"/>
				
		<!-- Where -->
		<GLOBAL_ATTRIBUTE NAME="area"/>
		<GLOBAL_ATTRIBUTE NAME="geospatial_lat_min" PATTERN="-?[0-9]+(\.[0-9]+)?.?"/>
		<GLOBAL_ATTRIBUTE NAME="geospatial_lat_max" PATTERN="-?[0-9]+(\.[0-9]+)?.?"/>
		<GLOBAL_ATTRIBUTE NAME="geospatial_lon_min" PATTERN="-?[0-9]+(\.[0-9]+)?.?"/>
		<GLOBAL_ATTRIBUTE NAME="geospatial_lon_max" PATTERN="-?[0-9]+(\.[0-9]+)?.?"/>
		<GLOBAL_ATTRIBUTE NAME="geospatial_vertical_min" PATTERN="-?[0-9]+(\.[0-9]+)?.?"/>
		<GLOBAL_ATTRIBUTE NAME="geospatial_vertical_max" PATTERN="-?[0-9]+(\.[0-9]+)?.?"/>
		
		<!-- When -->
		<GLOBAL_ATTRIBUTE NAME="time_coverage_start" PATTERN="[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(\.[0-9]+)?Z"/>
		<GLOBAL_ATTRIBUTE NAME="time_coverage_end" PATTERN="[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(\.[0-9]+)?Z"/>
		
		<!-- Who -->
		<GLOBAL_ATTRIBUTE NAME="institution"/>
		<GLOBAL_ATTRIBUTE NAME="institution_references"/>
		<GLOBAL_ATTRIBUTE NAME="sdn_edmo_code"/>	
		<GLOBAL_ATTRIBUTE NAME="contact"/>
		<GLOBAL_ATTRIBUTE NAME="author"/>
		<GLOBAL_ATTRIBUTE NAME="data_assembly_center"/>
		<GLOBAL_ATTRIBUTE NAME="principal_investigator"/>
		<GLOBAL_ATTRIBUTE NAME="principal_investigator_email"/>
		<GLOBAL_ATTRIBUTE NAME="observatory"/>
		<GLOBAL_ATTRIBUTE NAME="deployment_code"/>
		<GLOBAL_ATTRIBUTE NAME="deployment_label"/>

		<!-- How -->
		<GLOBAL_ATTRIBUTE NAME="distribution_statement"/>
		<GLOBAL_ATTRIBUTE NAME="citation"/>		
		<GLOBAL_ATTRIBUTE NAME="update_interval" PATTERN="hourly|daily|yearly|void"/>
		<GLOBAL_ATTRIBUTE NAME="qc_manual"/>

		<!-- ================== -->
		<!-- Optional variables -->
		<!-- ================== -->
		
		<!-- PHASE variables -->
		<!-- =============== -->

		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="PHASE"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="byte"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME" RANK="1"/>	
				<ATTRIBUTE NAME="long_name" VALUE="Glider trajectory phase code"/>
				<ATTRIBUTE NAME="conventions" VALUE="EGO reference table 9"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="-128"/>
				<ATTRIBUTE NAME="flag_values" VALUE_LIST="0||1||2||3||4||5||6"/>
				<ATTRIBUTE NAME="flag_meanings" VALUE="surface_drift descent subsurface_drift inflexion ascent grounded inconsistent"/>
			</MUST_VERIFY>
		</VARIABLE>
		
		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="PHASE_NUMBER"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="int"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME" RANK="1"/>	
				<ATTRIBUTE NAME="long_name" VALUE="Glider trajectory phase number"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="99999"/>
			</MUST_VERIFY>
		</VARIABLE>
			
		<!-- POSITIONING METHOD variable -->
		<!-- =========================== -->

		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="POSITIONING_METHOD"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="byte"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME" RANK="1"/>	
				<ATTRIBUTE NAME="long_name" VALUE="Positioning method"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="-128"/>
				<ATTRIBUTE NAME="flag_values" VALUE_LIST="0||1||2"/>
				<ATTRIBUTE NAME="flag_meanings" VALUE="GPS Argos interpolated"/>
			</MUST_VERIFY>
		</VARIABLE>	
		
		<!-- WATER CURRENTS variables -->
		<!-- ======================== -->
		
		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="WATERCURRENTS_JULD"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="double"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME_CURRENT" RANK="1"/>
				<ATTRIBUTE NAME="long_name" VALUE="Julian day (UTC) of the sub-surface current estimate"/>
				<ATTRIBUTE NAME="standard_name" VALUE="time"/>
				<ATTRIBUTE NAME="units" VALUE="days since 1950-01-01 00:00:00 UTC"/>
				<ATTRIBUTE NAME="conventions" VALUE="Relative julian days with decimal part (as parts of day)"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="999999"/>
			</MUST_VERIFY>
		</VARIABLE>
		
		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="WATERCURRENTS_LATITUDE"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="double"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME_CURRENT" RANK="1"/>	
				<ATTRIBUTE NAME="long_name" VALUE="Latitude of the sub-surface current estimate"/>
				<ATTRIBUTE NAME="standard_name" VALUE="latitude"/>
				<ATTRIBUTE NAME="units" VALUE="degree_north"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="99999"/>
				<ATTRIBUTE NAME="valid_min" VALUE="-90"/>
				<ATTRIBUTE NAME="valid_max" VALUE="90"/>
				<ATTRIBUTE NAME="axis" VALUE="Y"/>
			</MUST_VERIFY>
		</VARIABLE>		
		
		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="WATERCURRENTS_LONGITUDE"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="double"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME_CURRENT" RANK="1"/>	
				<ATTRIBUTE NAME="long_name" VALUE="Longitude of the sub-surface current estimate"/>
				<ATTRIBUTE NAME="standard_name" VALUE="longitude"/>
				<ATTRIBUTE NAME="units" VALUE="degree_east"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="99999"/>
				<ATTRIBUTE NAME="valid_min" VALUE="-180"/>
				<ATTRIBUTE NAME="valid_max" VALUE="180"/>
				<ATTRIBUTE NAME="axis" VALUE="X"/>				
			</MUST_VERIFY>
		</VARIABLE>	
		
		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="WATERCURRENTS_DEPTH"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="float"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME_CURRENT" RANK="1"/>	
				<ATTRIBUTE NAME="long_name" VALUE="Depth of the sub-surface current estimate"/>
				<ATTRIBUTE NAME="standard_name" VALUE="depth"/>
				<ATTRIBUTE NAME="units" VALUE="m"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="99999"/>
			</MUST_VERIFY>
		</VARIABLE>	
		
		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="WATERCURRENTS_U"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="float"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME_CURRENT" RANK="1"/>	
				<ATTRIBUTE NAME="long_name" VALUE="Eastward component of the sub-surface current estimate"/>
				<ATTRIBUTE NAME="standard_name" VALUE="eastward_sea_water_velocity"/>
				<ATTRIBUTE NAME="units" VALUE="cm/s"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="99999"/>
			</MUST_VERIFY>
		</VARIABLE>
		
		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="WATERCURRENTS_V"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="float"/>
				<DIMENSION_COUNT VALUE="1"/>
				<DIMENSION NAME="TIME_CURRENT" RANK="1"/>	
				<ATTRIBUTE NAME="long_name" VALUE="Northward component of the sub-surface current estimate"/>
				<ATTRIBUTE NAME="standard_name" VALUE="northward_sea_water_velocity"/>
				<ATTRIBUTE NAME="units" VALUE="cm/s"/>
				<ATTRIBUTE NAME="_FillValue" VALUE="99999"/>
			</MUST_VERIFY>
		</VARIABLE>				
		
		<!-- HISTORY variables -->
		<!-- ================= -->

		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="HISTORY_INSTITUTION"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="char"/>
				<DIMENSION_COUNT VALUE="2"/>
				<DIMENSION NAME="N_HISTORY" RANK="1"/>
				<DIMENSION NAME="STRING2" RANK="2"/>
				<ATTRIBUTE NAME="long_name" VALUE="Institution which performed action"/>
				<ATTRIBUTE NAME="conventions" VALUE="EGO reference table 4"/>
				<ATTRIBUTE NAME="_FillValue" VALUE=" "/>
			</MUST_VERIFY>
		</VARIABLE>
		
		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="HISTORY_STEP"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="char"/>
				<DIMENSION_COUNT VALUE="2"/>
				<DIMENSION NAME="N_HISTORY" RANK="1"/>
				<DIMENSION NAME="STRING4" RANK="2"/>
				<ATTRIBUTE NAME="long_name" VALUE="Step in data processing"/>
				<ATTRIBUTE NAME="conventions" VALUE="EGO reference table 12"/>
				<ATTRIBUTE NAME="_FillValue" VALUE=" "/>
			</MUST_VERIFY>
		</VARIABLE>
		
		<VARIABLE>
			<IDENTIFY>
				<NAME VALUE="HISTORY_SOFTWARE"/>
			</IDENTIFY>
			<MUST_VERIFY>
				<TYPE VALUE="char"/>
				<DIMENSION_COUNT VALUE="2"/>
				<DIMENSION NAME="N_HISTORY" RANK="1"/>
				<DIMENSION NAME="STRING8" RANK="2"/>
				<ATTRIBUTE NAME="long_name" VALUE="Name of software which performed action"/>
				<ATTRIBUTE NAME="conventions" VALUE="Institution dependent"/>
				<ATTRIBUTE NAME="_FillValue" VALUE=" "/>
			</MUST_VERIFY>
		</VARIABLE>
		
